<h1>Log in to track and share your workouts</h1>

<div class="field_with_errors">
  <p><strong><%= flash[:error] %></strong></p>
</div>

<%= form_for(:user) do |f|%>
  <%= f.label :username %>
  <%= f.text_field :username %><br>

  <%= f.label :password %>
  <%= f.password_field :password %><br>

  <%= f.submit "Log In" %>
<% end %>