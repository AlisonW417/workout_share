<h1>Sign up to track and share your workouts!</h1>

<%= render partial: 'layouts/errors', locals: {obj: @user} %>

<%= form_for @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %><br>

    <%= f.label :password %>
    <%= f.password_field :password %><br>

    <%= f.submit "Sign Up" %>
<% end %>